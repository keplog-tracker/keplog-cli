{
  "name": "@keplog/cli",
  "version": "1.0.0",
  "description": "Official Keplog CLI for uploading source maps and managing projects",
  "main": "dist/index.js",
  "bin": {
    "keplog": "./bin/keplog"
  },
  "scripts": {
