    "ora": "^5.4.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/form-data": "^2.2.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.5",
    "@types/prompts": "^2.4.9",
    "axios-mock-adapter": "^2.1.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
